{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "nelmio/cors-bundle": "^2.1.0",
        "sensio/framework-extra-bundle": "^v5.6.1",
        "symfony/apache-pack": "^v1.0.1",
        "symfony/asset": "v5.1.7",
        "symfony/console": "v5.1.7",
        "symfony/dotenv": "v5.1.7",
        "symfony/flex": "v1.9.9",
        "symfony/form": "v5.1.7",
        "symfony/framework-bundle": "v5.1.7",
        "symfony/google-mailer": "v5.1.7",
        "symfony/http-client": "v5.1.7",
        "symfony/mailer": "v5.1.7",
        "symfony/maker-bundle": "v1.9.0",
        "symfony/monolog-bundle": "^v3.6.0",
        "symfony/orm-pack": "^v2.0.0",
        "symfony/security-bundle": "v5.1.7",
        "symfony/serializer": "v5.1.7",
        "symfony/serializer-pack": "^v1.0.4",
        "symfony/validator": "v5.1.7",
        "symfony/yaml": "v5.1.7",
        "twig/cssinliner-extra": "^v3.0.5",
        "twig/extra-bundle": "^v3.0.5"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^v2.4.1",
        "symfony/phpunit-bridge": "^v5.1.7",
        "symfony/profiler-pack": "^v1.0.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
